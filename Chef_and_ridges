//Chef and ridges problem of November Long challenge 
#include <stdio.h>
#include <math.h>

int main()
{
	int T,N,s;
	int maxn=1000005;
	scanf("%d",&T);
	int a[maxn];
	a[1]=1;
	for (int i = 2; i <= maxn; i++)
	{
		/* code */
		a[i]=pow(2,i-1)-a[i-1];
	}
    while(T--)
	   {
		  
		scanf("%d",&N);
		s=pow(2,N);
		printf("%d %d ",a[N],s);
	   }

	return 0;
}
